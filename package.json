{
  "name": "moviola",
  "version": "0.0.1",
  "description": "Training react app",
  "main": "server/index.js",
  "repository": "https://github.com/mateuszkrzak/moviola.git",
  "author": "matkrzak@outlook.com",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./server",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack",
    "prod": "cross-env NODE_ENV=production node ./server",
    "test": "jest",
    "test:watch": "jest --watch",
    "e2e": "cypress open"
  },
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": ["js", "jsx"],
    "setupFiles": ["./app/test-setup.js"],
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "moduleDirectories": ["./app/src", "node_modules"],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/src/**/*.{js,jsx}",
      "!app/src/**/*.spec.{js,jsx}",
      "!app/src/bootstrap.jsx"
    ],
    "coverageReporters": ["lcov", "text-summary"],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "express": "^4.16.3",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "typeface-roboto": "0.0.54",
    "url-search-params": "^0.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-css-modules": "^3.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "cypress": "^2.1.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.2.0",
    "node-sass": "^4.9.0",
    "postcss-scss": "^1.0.5",
    "prettier-eslint": "^8.8.1",
    "react-hot-loader": "^4.3.0",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1",
    "regenerator-runtime": "^0.11.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-middleware": "3.1.3",
    "webpack-dev-server": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
